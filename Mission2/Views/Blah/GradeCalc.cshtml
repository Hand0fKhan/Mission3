@{
}

@model GradeCalcModel;

<h2>Grade Calculator</h2>
<form method="post">
    <label>Assignments: </label><input id="assign" asp-for="Assignments" type ="number" min="1" max="100"/><br />
    <label>Group Projects: </label><input id="groupProj" asp-for="Projects" type ="number" min="1" max="100"/><br />
    <label>Quizzes: </label><input id="quiz" asp-for="Quiz" type ="number" min="1" max="100"/><br />
    <label>Exams: </label><input id="test" asp-for="Test" type ="number" min="1" max="100"/><br />
    <label>INTEX: </label><input id="intex" asp-for="Intex" type ="number" min="1" max="100"/><br />
    <input id="calcBtn" type="submit" value="Calculate" />
       
    <div asp-validation-summary="All"></div>
</form>

<script src="js/GradeCalc.js"></script>